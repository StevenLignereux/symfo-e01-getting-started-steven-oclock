{% extends 'base.html.twig' %}

{% block body %}

<h1>Liste des étudiants</h1>

<ul>
  {% for key, student in students %}
      <li><a href="{{path('student_show', {'id': key}) }}">{{ student.name }}</a></li>
      {% else %}
          <li>Aucun étudiant.</li>
  {% endfor %}
</ul>



<a href="{{ path('student_pdf') }}">Télécharger le planning de la formation</a>

{% endblock %}
